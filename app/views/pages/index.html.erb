<!-- CONTACT -->
<section id="contact">
	<div class="container">

		<header class="text-center mb-60">
			<h1>Contact Us</h1>
			<p class="lead font-lato">Thank you for your interest in learning more about Rocket Elevators.<br>
				We welcome your comments and questions.</p>
			<hr />
		</header>


		<div class="row">

			<!-- FORM -->
			<div class="col-md-8 col-sm-8">

				<h3>Drop us a line or just say <strong><em>Hello!</em></strong></h3>						
				

				<!-- Alert Success -->
				<div id="alert_success" class="alert alert-success mb-30">
					<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
					<strong>Thank You!</strong> Your message successfully sent!
				</div><!-- /Alert Success -->


				<!-- Alert Failed -->
				<div id="alert_failed" class="alert alert-danger mb-30">
					<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
					<strong>[SMTP] Error!</strong> Internal server error!
				</div><!-- /Alert Failed -->


				<!-- Alert Mandatory -->
				<div id="alert_mandatory" class="alert alert-danger mb-30">
					<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
					<strong>Sorry!</strong> You need to complete all mandatory (*) fields!
				</div><!-- /Alert Mandatory -->


				<form action="/leads" method="post" id="form-contact" enctype="multipart/form-data">
					<fieldset>
					   <input type="hidden" name="action" value="contact_send" />

						<div class="row">
								<div class="col-md-4">
									<label for="contact:name">Full Name *</label>
									<input required type="text" value="" class="form-control" name="leads[full_name_of_contact]" id="contact:name">
								</div>
								<div class="col-md-4">
									<label for="contact:email">E-mail Address *</label>
									<input required type="email" value="" required placeholder="exemple@company.com" class="form-control" name="leads[email]" id="contact:email">
								</div>
								<div class="col-md-4">
									<label for="contact:phone">Phone *</label>
									<input type="tel" value="" required placeholder="(555)555-5555" class="form-control" name="leads[phone]" id="contact:phone" pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}|[0-9]{3} [0-9]{3} [0-9]{4}|[0-9]{3}[0-9]{3}[0-9]{4}">
								</div>
								<div class="col-md-4">
									<label for="contact:enterprise_name">Enterprise Name *</label>
									<input required type="text" value="" class="form-control" name="leads[company_name]" id="contact:enterprise_name">
								</div>
								<div class="col-md-4">
									<label for="contact:project_name">Project Name *</label>
									<input required type="text" value="" class="form-control" name="leads[project_name]" id="contact:project_name">
								</div>
						</div>
						<div class="row">
								<div class="col-md-8">
									<label for="contact:description">Project Description *</label>
									<input required type="text" value="" class="form-control" name="leads[project_description]" id="contact:description">
								</div>
								<div class="col-md-4">
									<label for="contact_department">Department</label>
									<select class="form-control pointer" name="leads[department_in_charge_of_elevators]">
										<option value="Sales">Sales</option>
										<option value="Support">Support</option>
										<option value="Administration">Administration</option>
									</select>
								</div>
						</div>
						<div class="row">
								<div class="col-md-12">
									<label for="contact:message">Message *</label>
									<textarea required maxlength="5000" rows="8" class="form-control" name="leads[message]" id="contact:message"></textarea>
								</div>
						</div>
						<div class="row" id="aFile_upload">
								<div class="col-md-12">
									<label for="contact:attachment">File Attachment</label>
									<!-- custom file upload -->
									<input class="custom-file-upload" type="file" name="attachment" id="contact:attachment" data-btn-text="Select a File" accept=".zip,.pdf,.jpg,.png,.txt" />
									<small class="text-muted block">Max file size: 10Mb (zip/pdf/jpg/png/txt)</small>
									
								</div>
						</div>

					</fieldset>

					<div class="row">
						<div class="col-md-12">
							<button type="submit" class="btn btn-primary"><i class="fa fa-check" ></i> SEND MESSAGE</button>
							<%# <script>
								function sendingAlert(onclick="sendingAlert()") {
									alert("Thank you for sending us a message. We will be back to you shortly!");
								}
							</script> %>
						</div>
					</div>
				</form>

			</div>
			<!-- /FORM -->


			<!-- INFO -->
			<div class="col-md-4 col-sm-4">

				<h2>Visit Us</h2>
				<div id="map2" class="h-300"><iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2729.0431801755108!2d-71.28188348416968!3d46.84283987914146!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4cb897d0e1cafe3f%3A0x9863b2d992c71670!2sCodeBoxx!5e0!3m2!1sen!2sus!4v1613771227185!5m2!1sen!2sus" width="290" height="300" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe></div>

				<hr />

				<p>
					<span class="block"><strong><i class="fa fa-map-marker"></i> Address:</strong> 418-725 Boulevard Lebourgneuf,<br> Québec, QC, G2J 0C4, Canada</span>
					<span class="block"><strong><i class="fa fa-phone"></i> Phone:</strong> <a href="tel:(418)555-1234">(418) 555-1234</a></span>
					<span class="block"><strong><i class="fa fa-envelope"></i> Email:</strong> <a href="mailto:info@codeboxx.biz">info@codeboxx.biz</a></span>
				</p>

			</div>
			<!-- /INFO -->

		</div>

	</div>
</section>
